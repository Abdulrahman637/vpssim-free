}
if [ -f /root/vpssim-nang-cap-nginx ]; then
rm -rf /root/vpssim-nang-cap-nginx
fi
#wget –no-check-certificate https://www.echbay.com/download/vpssim-nang-cap-nginx ; chmod +x vpssim-nang-cap-nginx ; bash vpssim-nang-cap-nginx
